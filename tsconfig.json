{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@environments/*": [
        "environments/*"
      ],
      "@collections/*": [
        "src/collections/*"
      ],
      "@dashboard/*": [
        "src/dashboard/*"
      ],
      "@playlist/*": [
        "src/playlist/*"
      ],
      "@search/*": [
        "src/search/*"
      ],
      "@tracks/*": [
        "src/tracks/*"
      ]
      
    },
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "esnext.asynciterable"
    ],
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeChecking": true,
    "strictInjectionParameters": true,
    "strictTemplates": true,
    "noImpicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  }
}