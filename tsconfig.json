{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app-core/*": [
        "src/app/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@guards/*": [
        "src/app/guards/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@pipes/*": [
        "src/app/pipes/*"
      ],
      "@queries/*": [
        "src/app/queries/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ]
    },
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "esnext.asynciterable"
    ],
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "fullTemplateTypeChecking": true,
    "strictInjectionParameters": true,
    "strictTemplates": true,
    "noImpicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  }
}